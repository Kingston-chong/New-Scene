{
  "name": "dcl-project",
  "version": "1.0.0",
  "description": "SDK7 Template scene",
  "scripts": {
    "start": "CI=true dcl start -p ${PORT:=8000}",
    "deploy": "sdk-commands deploy",
    "build": "sdk-commands build",
    "upgrade-sdk": "npm install --save-dev @dcl/sdk@latest",
    "upgrade-sdk:next": "npm install --save-dev @dcl/sdk@next",
    "watch": "dcl build --watch"
  },
  "devDependencies": {
    "@dcl/js-runtime": "7.8.7",
    "@dcl/sdk": "^7.8.7",
    "@dcl/ui-scene-utils": "^1.8.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "none"
  },
  "bundledDependencies": [
    "@dcl/ui-scene-utils"
  ],
  "dependencies": {
    "@dcl/asset-packs": "^2.4.0",
    "@zeroxwork/decentraland-virtual-browser-client": "^1.0.8",
    "decentraland": "^3.21.0",
    "decentraland-ui2": "^0.19.0"
  },
  "bundleDependencies": [
    "@dcl/ui-scene-utils"
  ]
}
